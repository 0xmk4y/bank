          'src',
          '//tms.capitalone.com/capitalone/' +
            window._sequoia.scriptEnv +
            '/Bootstrap.js'
        );
        script.addEventListener('load', function () {
          function publishNRError(eventName) {
            if (window.newrelic && newrelic.noticeError) {
              newrelic.noticeError('ensighten event called with ' + eventName);
